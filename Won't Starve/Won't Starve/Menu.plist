	<dict>
		<key>name</key>
		<string>曼德拉草汤</string>
		<key>icon</key>
		<string>Mandrake_Soup</string>
		<key>health</key>
		<string>+100</string>
		<key>hungry</key>
		<string>+150</string>
		<key>sanity</key>
		<string>+5</string>
		<key>badTime</key>
		<string>6</string>
